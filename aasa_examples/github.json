        "details": [
            {
                "appIDs": [
                    "VEKTX9H2N7.com.github.stormbreaker.dev",
                    "VEKTX9H2N7.com.github.stormbreaker.prod"
                ],
                "components": [
                    {
                        "/": "/",
                        "exclude": true,
                        "comment": "Matches the GitHub.com landing page"
                    },
                    {
                        "/": "/orgs/*/$(includedOrgPages)/*",
                        "exclude": false,
                        "comment": "Matches handleable org products and their subpages"
                    },
                    {
                        "/": "/users/*/projects/*",
                        "exclude": false,
                        "comment": "Matches org projects and their subpages"
                    },
                    {
                        "/": "/$(excludedProducts)",
                        "exclude": true,
                        "comment": "Matches GitHub product pages"
                    },
                    {
                        "/": "/$(excludedProducts)/*",
                        "exclude": true,
                        "comment": "Matches GitHub product subroutes"
                    },
                    {
                        "/": "/*/*/$(excludedRepoPages)",
                        "exclude": true,
                        "comment": "Matches unsupported GitHub repo pages"
                    },
                    {
                        "/": "/*/*/$(excludedRepoPages)/*",
                        "exclude": true,
                        "comment": "Matches unsupported GitHub repo subroutes"
                    },
                    {
                        "/": "/*/*/$(excludedRepoSearchPages)",
                        "?": {
                            "q": "?*"
                        },
                        "exclude": true,
                        "comment": "Matches unsupported GitHub repo pages"
                    },
                    {
                        "/": "/*",
                        "?": {
                            "tab": "?*"
                        },
                        "exclude": true,
                        "comment": "Disable links to specific tabs on profiles"
                    },
                    {
                        "/": "*",
                        "comment": "Matches all remaining routes"
                    }
                ]
            }
        ]
    }
